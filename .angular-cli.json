{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "ab-project",
    "npmScope": "ab-project",
    "latestMigration": "20171213-update-tsconfig-spec-to-exclude-e2e"
  },
  "apps": [
    {
      "name": "app0",
      "root": "apps/app0/src",
      "outDir": "dist/apps/app0",
      "assets": [
        "assets",
        "../../../assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../../../node_modules/datatables.net-dt/css/jquery.dataTables.css",
        "../../../node_modules/font-awesome-scss/scss/font-awesome.scss",
        "./../../../styles/style.scss"
      ],
      "scripts": [
        "../../../node_modules/jquery/dist/jquery.min.js",
        "../../../node_modules/bootstrap/dist/js/bootstrap.bundle.js",
        "../../../node_modules/datatables.net/js/jquery.dataTables.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "root",
      "root": "libs/root/src",
      "test": "../../../test.ts",
      "appRoot": ""
    },
    {
      "name": "router-state",
      "root": "libs/router-state/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "scan",
      "root": "libs/scan/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "sub-header",
      "root": "libs/sub-header/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "widgets",
      "root": "libs/widgets/src",
      "test": "../../../test.js",
      "appRoot": ""
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/app0/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/app0/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    }
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
